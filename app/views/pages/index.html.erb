<script>
(function() {

    var quotes = $(".quotes");
    var quoteIndex = -1;

    function showNextQuote() {
        ++quoteIndex;
        quotes.eq(quoteIndex % quotes.length)
            .fadeIn(1500)
            .delay(1000)
            .fadeOut(1500, showNextQuote);
    }

    showNextQuote();

})();
</script>
